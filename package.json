{
  "name": "yuque-tools",
  "version": "2.1.1",
  "description": "语雀知识库内容批量导出和二次处理工具",
  "keywords": [
    "yuque",
    "yuque export",
    "yuque tools",
    "yuque-tools"
  ],
  "homepage": "https://github.com/vannvan/yuque-tools#readme",
  "repository": {
    "type": "git",
    "url": "git@github.com:vannvan/yuque-tools.git"
  },
  "license": "MIT",
  "author": "vannvan",
  "type": "module",
  "exports": {
    ".": {
      "types": {
        "default": "./sdk.d.ts"
      },
      "default": {
        "default": "./dist/sdk/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "types": "sdk.d.ts",
  "bin": {
    "ytool": "dist/cli/index.js"
  },
  "files": [
    "package.json",
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "rimraf coverage dist tmp",
    "cli": "ts-node-esm -T src/cli/index.ts",
    "pack:macos": "pkg . --out-path=release/ --targets=macos --compress=GZip",
    "pack:win": "pkg . --out-path=release/ --targets=win --compress=GZip",
    "tsc": "npm run clean && tsc -p tsconfig.release.json",
    "watch": "tsc --watch -p tsconfig.json"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "chalk": "4.1.2",
    "commander": "^10.0.0",
    "glob": "7",
    "inquirer": "8",
    "jsdom": "^21.1.1",
    "jsencrypt-node": "^1.0.4",
    "latest-version": "^7.0.0",
    "ora": "4",
    "pkg": "^5.8.1",
    "rimraf": "^5.0.1",
    "terminal-link": "^3.0.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.3"
  },
  "devDependencies": {
    "@artus/tsconfig": "^1.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/jest": "^29.5.3",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.15.11",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  },
  "publishConfig": {
    "vannvan:registry": "https://npm.pkg.github.com/"
  },
  "binName": "ytool"
}